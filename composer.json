{
    "name": "TheNandan/TheLogger",
    "description": "A logger package for laravel.",
    "license": "license",
    "authors": [
        {
            "name": "Keshari Nandan",
            "email": "thenandan4@gmail.com",
            "homepage": "https://github.com/thenandan"
        }
    ],
    "homepage": "https://github.com/thenandan/thelogger",
    "keywords": ["Laravel", "thelogger"],
    "require": {
        "illuminate/support": "~5",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TheNandan\\TheLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheNandan\\TheLogger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheNandan\\TheLogger\\TheLoggerServiceProvider"
            ],
            "aliases": {
                "thelogger": "TheNandan\\TheLogger\\Facades\\TheLogger"
            }
        }
    }
}
